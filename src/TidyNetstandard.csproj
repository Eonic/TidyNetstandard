<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <RootNamespace>Tidy</RootNamespace>
    <TargetFramework>netstandard1.0</TargetFramework>
    <Description>.Net Standard Version of Dave Ragget's Tidy Library based on Tidy.NET (http://sourceforge.net/projects/tidynet/) with HTML5 tags added as implemented in ProteanCMS. Thank you to ziet200</Description>
    <PackageLicenseExpression>W3C</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/eonic/TidyNetstandard</PackageProjectUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.3</Version>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>TidyNetstandard.snk</AssemblyOriginatorKeyFile>
    <NeutralLanguage>en</NeutralLanguage>
    <Title>HTMLTidy .Net Standard</Title>
    <Authors>Trevor Spink</Authors>
    <Company>Eonic Digital LLP.</Company>
    <PackageId>TidyNetStandard5</PackageId>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>
  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>
</Project>